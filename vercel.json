{
  "version": 2,
  "buildCommand": "node scripts/inject-api-url.js",
  "outputDirectory": "frontend",
  "routes": [
    {
      "src": "/ui/(.*)",
      "dest": "/$1"
    },
    {
      "src": "/ui",
      "dest": "/index.html"
    },
    {
      "src": "/(.*)",
      "dest": "/index.html"
    }
  ]
}

